{% extends "_layout.twig" %}
    
{% block content %}
    <h1>{{ entry.title }}</h1>
    {% set blocks = entry.vacatureInformatie.all() %}
        {% for block in blocks %}
            <h2>{{ block.sectionTitle }}</h2>
            <h3>{{ block.subTitle }}</h3>
            <p>{{ block.sectionContent }}</p>
        {% endfor %}
{% endblock %}